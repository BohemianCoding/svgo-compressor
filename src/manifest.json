{
  "compatibleVersion": 3.8,
  "bundleVersion": 1,
  "identifier": "com.sketchapp.plugins.svgo-compressor",
  "icon": "svgo-compressor.png",
  "commands": [
    {
      "name": "About SVGO Compressor",
      "identifier": "svgCompress",
      "script": "./plugin.js",
      "handlers": {
        "run" : "openSettings",
        "actions": {
          "ExportSlices": "compress",
          "Export": "compress"
        }
      }
    },
    {
      "name": "Copy optimized SVG code",
      "identifier": "svgCopyCompressed",
      "script": "./plugin.js",
      "handler":"svgCopyCompressed",
      "shortcut": "cmd ctrl shift s"
    }
  ]
}
